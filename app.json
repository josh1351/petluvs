{
  "expo": {
    "name": "pevluvsapp",
    "description": "PetLuvs Native App",
    "slug": "lpevluvsapp",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anyalpha.pevluvsapp"
    },
    "android": {
      "package": "com.anyalpha.pevluvsapp"
    }
  }
}
